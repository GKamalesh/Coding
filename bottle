



#include<stdio.h>
int main()
{
    long long int i,j,c=0,e,k=0,l=0,n,a[1000],d[100000],f[100000],s,r,m=0;
    scanf("%lld",&n);
    for(i=1;i<=n;i++)   
    scanf("%lld",&a[i]);     
    for(i=1;i<=n;i++)   
    {
        j=i;        
        e=0;            
        while(j<=n)      
        {
            d[k]=j;          
            j+=a[j];            
            e++;    
            f[l]=e;          
            k++;  
        }
        d[k]=0; 
        k++;        
        l++;    
    }
    s=f[0];
    e=0;    
    for(i=0;i<l;i++)    
    {
        if(f[i]>s)  
        {   
            s=f[i]; 
            e=i;    
        }
    }
    for(i=0;i<=k;i++)
    {
        if(d[i]==0) 
        m++;
        if(d[i]==0 && m==e)  
        c=i;
        if(d[i]==0 && m==e+1)     
        s=i;
    }
    for(i=c;i<s;i++)
    {
        if(a[(d[i])]!=0)
    printf("%lld ",a[(d[i])]); 
    }
    return 0;
}
