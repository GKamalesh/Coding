#include <stdio.h>
int main()
{
   long int n,a[1000],i=0,j;
    scanf("%ld",&n);
    while(n>0)
    {
        a[i]=n%10;
        i+=1;;
        n=n/10;
    }
    for(j=i-1;j>=0;j--)
    {
        if(a[j]==1)
        printf("a");
        else if(a[j]==2)
        printf("b");
        else if(a[j]==3)
        printf("c");
        else if(a[j]==4)
        printf("d");
        else if(a[j]==5)
        printf("e");
        else if(a[j]==6)
        printf("f");
        else if(a[j]==7)
        printf("g");
        else if(a[j]==8)
        printf("h");
        else
        printf("i");
    }
}
